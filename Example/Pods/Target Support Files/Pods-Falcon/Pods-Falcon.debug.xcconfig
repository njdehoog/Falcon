GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Falcon" "${PODS_ROOT}/Headers/Public/Mantle"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Falcon" -isystem "${PODS_ROOT}/Headers/Public/Mantle"
OTHER_LDFLAGS = -ObjC -l"Pods-Falcon-Falcon" -l"Pods-Falcon-Mantle" -framework "Foundation"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods